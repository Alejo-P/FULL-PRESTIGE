          <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">// Verifica si existe un token y el cargo del empleado
if (vars.get(&quot;authToken&quot;) != null &amp;&amp; vars.get(&quot;nombreEmpleado&quot;) != null) {
    // Almacena el token y el nombre del empleado globalmente 
    props.put(&quot;TOKEN_&quot; + vars.get(&quot;nombreEmpleado&quot;), vars.get(&quot;authToken&quot;));
    props.put(&quot;NOMBRE_GLOBAL&quot;, vars.get(&quot;nombreEmpleado&quot;));
    log.info(&quot;Token almacenado correctamente para el cargo: Administrador&quot;);
} else {
    // Manejo de errores: token no encontrado o cargo no coincide
    if (vars.get(&quot;authToken&quot;) == null) {
        log.error(&quot;El token no se pudo extraer correctamente.&quot;);
    }
    if (vars.get(&quot;nombreEmpleado&quot;) == null) {
        log.error(&quot;El nombre del empleado no se pudo extraer correctamente.&quot;);
    }
}

if (vars.get(&quot;cargoEmpleado&quot;) != null) {
    props.put(&quot;CARGO_GLOBAL&quot;, vars.get(&quot;cargoEmpleado&quot;));
} else {
    log.error(&quot;El cargo del empleado no se pudo extraer correctamente.&quot;);
}</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
